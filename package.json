{
	"name": "Nudge",
	"version": "0.2.2",
	"description": "",
	"main": "index.js",
	"author": {
		"name": "Abhinav Robinson",
		"email": "abhinavrobinson@gmail.com"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"postinstall": "electron-builder install-app-deps",
		"rebuild-deps": "electron-builder install-app-deps",
		"dev": "electron-webpack dev",
		"build": "electron-webpack && electron-builder --publish always",
		"release": "electron-webpack && electron-builder -m --publish always",
		"release:win": "electron-webpack && electron-builder -w --publish always"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/aniketbiprojit/process-timer"
	},
	"keywords": [],
	"license": "ISC",
	"devDependencies": {
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@babel/types": "^7.11.0",
		"@reduxjs/toolkit": "^1.5.0",
		"@types/axios": "^0.14.0",
		"@types/babel__core": "^7.1.9",
		"@types/chai": "^4.2.12",
		"@types/electron": "^1.6.10",
		"@types/mocha": "^8.0.3",
		"@types/node": "^14.6.0",
		"@types/react": "^16.9.48",
		"@types/react-router": "^5.1.11",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"@welldone-software/why-did-you-render": "^6.0.5",
		"babel": "^6.23.0",
		"babel-cli": "^6.26.0",
		"chai": "^4.2.0",
		"electron": "^12.0.0-beta.11",
		"electron-builder": "^22.8.0",
		"electron-redux": "^1.5.4",
		"electron-webpack": "^2.8.2",
		"electron-webpack-ts": "^4.0.1",
		"eslint": "^7.18.0",
		"eslint-plugin-react": "^7.22.0",
		"husky": "^4.3.6",
		"prettier": "^2.2.1",
		"pretty-quick": "^3.1.0",
		"react-redux": "^7.2.2",
		"redux": "^4.0.5",
		"ts-node": "^9.0.0",
		"typesafe-actions": "^5.1.0",
		"typescript": "^4.0.2",
		"webpack": "^4.44.1"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.32",
		"@fortawesome/free-solid-svg-icons": "^5.15.1",
		"@fortawesome/react-fontawesome": "^0.1.14",
		"@types/firebase": "^3.2.1",
		"@types/socket.io-client": "^1.4.34",
		"active-win": "^6.2.0",
		"agora-rtc-sdk": "^3.3.1",
		"agora-rtc-sdk-ng": "^4.2.1",
		"axios": "^0.21.1",
		"electron-log": "^4.3.1",
		"electron-store": "^6.0.1",
		"electron-updater": "^4.3.5",
		"github-api": "^3.3.0",
		"node-mac-permissions": "^2.2.0",
		"node-sass": "4.14.1",
		"peerjs": "^1.3.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-router": "^4.3.1",
		"react-router-dom": "^5.2.0",
		"react-tooltip": "^4.2.13",
		"reflect-metadata": "^0.1.13",
		"sass-loader": "^10.1.0",
		"socket.io-client": " ^2.3.1",
		"source-map-support": "^0.5.19",
		"typedi": "^0.8.0"
	},
	"electronWebpack": {
		"main": {
			"sourceDirectory": "./"
		},
		"renderer": {
			"sourceDirectory": "app/"
		}
	},
	"build": {
		"appId": "app.atom.nudge",
		"productName": "Nudge",
		"files": [
			"!amplify/"
		],
		"publish": [
			{
				"provider": "s3",
				"bucket": "nudge-testing"
			}
		],
		"snap": {
			"publish": [
				{
					"provider": "s3",
					"bucket": "nudge-testing"
				}
			]
		},
		"linux": {
			"artifactName": "${productName}-${os}-${channel}-${version}.${ext}",
			"category": "Utility",
			"icon": "build/icons/",
			"target": [
				"snap",
				"deb"
			]
		},
		"win": {
			"artifactName": "${productName}-${os}-${channel}-${version}.${ext}",
			"compression": "store",
			"icon": "build/icons/",
			"target": [
				"nsis"
			]
		},
		"mac": {
			"artifactName": "${productName}-${os}-${channel}-${version}.${ext}",
			"category": "public.app-category.developer-tools",
			"target": [
				"dmg",
				"zip"
			],
			"icon": "build/icons/"
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
