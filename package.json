{
	"name": "Electron",
	"version": "0.0.5",
	"description": "",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"postinstall": "electron-builder install-app-deps",
		"rebuild-deps": "electron-builder install-app-deps",
		"dev": "electron-webpack dev",
		"build": "electron-webpack && electron-builder",
		"release": "electron-webpack && electron-builder -m --publish always",
		"release:win": "electron-webpack && electron-builder -w"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/AbhinavRobinson/process-timer"
	},
	"keywords": [],
	"license": "ISC",
	"devDependencies": {
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@babel/types": "^7.11.0",
		"@types/babel__core": "^7.1.9",
		"@types/chai": "^4.2.12",
		"@types/electron": "^1.6.10",
		"@types/mocha": "^8.0.3",
		"@types/node": "^14.6.0",
		"@types/react": "^16.9.48",
		"@types/react-dom": "^16.9.8",
		"@types/react-router-dom": "^5.1.6",
		"babel": "^6.23.0",
		"babel-cli": "^6.26.0",
		"chai": "^4.2.0",
		"electron": "^10.1.0",
		"electron-builder": "^22.8.0",
		"electron-webpack": "^2.8.2",
		"electron-webpack-ts": "^4.0.1",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.2",
		"webpack": "^4.44.1"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.32",
		"@fortawesome/free-solid-svg-icons": "^5.15.1",
		"@fortawesome/react-fontawesome": "^0.1.14",
		"@types/firebase": "^3.2.1",
		"active-window": "^0.1.0",
		"electron-log": "^4.2.4",
		"electron-store": "^6.0.1",
		"electron-updater": "^4.3.5",
		"firebase": "^8.2.1",
		"github-api": "^3.3.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-router": "^4.3.1",
		"react-router-dom": "^5.2.0",
		"source-map-support": "^0.5.19"
	},
	"electronWebpack": {
		"main": {
			"sourceDirectory": "./"
		},
		"renderer": {
			"sourceDirectory": "app/"
		}
	},
	"build": {
		"linux": {
			"artifactName": "${productName}-${os}-${channel}-${version}.${ext}",
			"category": "Utility",
			"icon": "build/icons/",
			"target": [
				"AppImage"
			]
		},
		"win": {
			"artifactName": "${productName}-${os}-${channel}-${version}.${ext}",
			"compression": "store",
			"icon": "build/icons/",
			"target": [
				"nsis"
			],
			"publish": [
				{
					"provider": "github"
				}
			]
		},
		"mac": {
			"artifactName": "${productName}-${os}-${channel}-${version}.${ext}",
			"category": "public.app-category.developer-tools",
			"target": [
				"dmg"
			],
			"icon": "build/icons/"
		}
	}
}
