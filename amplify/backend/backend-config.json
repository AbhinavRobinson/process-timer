{
	"function": {
		"atomutils": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "agoraChannelState",
					"attributes": ["Name", "Arn", "StreamArn"]
				}
			]
		}
	},
	"api": {
		"mainApi": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "atomutils",
					"attributes": ["Name", "Arn"]
				}
			]
		}
	},
	"storage": {
		"agoraChannelState": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	}
}
